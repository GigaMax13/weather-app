{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "debugger": "REACT_DEBUGGER=\"unset ELECTRON_RUN_AS_NODE && open -g 'rndebugger://set-debugger-loc?port=19000' ||\" expo start --clear --dev --lan",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "start": "expo start",
    "eject": "expo eject",
    "coverage": "jest --coverage",
    "watch": "jest --watch",
    "test": "jest",
    "clear": "npm run clear:temp && npm run clear:watchman && npm run clear:modules",
    "clear:temp": "rm -rf $TMPDIR/haste-* && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/react-*",
    "clear:modules": "rm -rf node_modules && npm i && npm audit fix",
    "clear:watchman": "watchman watch-del-all"
  },
  "dependencies": {
    "@expo/vector-icons": "^12.0.0",
    "axios": "^0.21.1",
    "dayjs": "^1.10.1",
    "expo": "~40.0.0",
    "expo-constants": "~9.3.3",
    "expo-location": "~10.0.0",
    "expo-permissions": "~10.0.0",
    "expo-splash-screen": "~0.8.0",
    "expo-status-bar": "~1.0.3",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-40.0.1.tar.gz",
    "react-native-appearance": "~0.3.3",
    "react-native-dotenv": "^2.4.3",
    "react-native-web": "~0.13.12",
    "styled-components": "^5.2.1",
    "swr": "^0.3.11"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "~25.2.6",
    "babel-preset-expo": "^8.3.0",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "~25.2.6",
    "jest-expo-enzyme": "^1.0.4",
    "jest-styled-components": "^7.0.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "~16.13.1"
  },
  "jest": {
    "preset": "jest-expo-enzyme",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "setupFiles": [
      "dotenv/config"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
